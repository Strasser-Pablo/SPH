#include "position.h"
template<int N=3>
class Position{
  Position();
public:
  double Distance(Position<N> A);
};

template<>
class Position<3>{
 double m_x; 
  double m_y;
  double m_z;
public:
  Position();
    Position(double x,double y,double z);
double Distance(Position<3> A);
};